{
  "extends": "tsconfig-moon/tsconfig.projects.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "checkJs": false,
    "strict": true,
    "target": "ES6",
    "module": "ESNext",
    "verbatimModuleSyntax": false,
    "emitDeclarationOnly": false,
    "sourceMap": true,
    "noUnusedLocals": false,
    "rootDir": "dist_old",
    "tsBuildInfoFile": "dist_old/.tsbuildinfo",
    "allowJs": false,
    "noImplicitOverride": false,
    "outDir": "dist_old",
  }
}
